<ActorFrame
MadeChoiceP1MessageCommand="playcommand,shutup;playcommand,GoOff"
MadeChoiceP2MessageCommand="playcommand,shutup;playcommand,GoOff"
shutupCommand="%function() SHUTUPSERIOUSLY() end"
>
<children>
<Layer
Class="Quad"
OnCommand="diffuse,0,0,0,0;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_HEIGHT;linear,.1;diffuse,1,1,1,1;"
InitCommand="draworder,100"
/>
<Layer
Class="Quad"
OnCommand="diffuse,0,0,0,0;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_HEIGHT;sleep,2;linear,.4;diffuse,#000000"
InitCommand="draworder,152"
/> 
<Layer
Class="Quad"
OnCommand="diffuse,0,0,0,0;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_HEIGHT;sleep,1;linear,.6;diffuse,0,0,0,0;"
InitCommand="draworder,150"
/>

<Layer
Class="Autoactor"
File="_itg3"
OnCommand="diffusealpha,0;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-2;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_WIDTH/6.4;linear,1.2;diffusealpha,1;"
InitCommand="draworder,117"
/>
<Layer
Class="Autoactor"
File="blueflare"
OnCommand="additiveblend,1;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomx,15;zoomtoheight,SCREEN_HEIGHT+SCREEN_HEIGHT/4;decelerate,1.1;zoomtoheight,0;diffusealpha,.2;"
InitCommand="draworder,115"
/>
<Layer
Class="Autoactor"
File="fire"
OnCommand="additiveblend,1;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_WIDTH/6.4;sleep,.8;linear,.5;diffusealpha,0;"
InitCommand="draworder,117"
/>
<Layer
Class="Autoactor"
File="fire"
OnCommand="additiveblend,1;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_WIDTH/6.4;sleep,.8;linear,.5;diffusealpha,0;"
InitCommand="draworder,117"
/>


<Layer
Class="Autoactor"
OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-SCREEN_HEIGHT;decelerate,.25;y,SCREEN_CENTER_Y-SCREEN_HEIGHT/4;"
File="top"
InitCommand="draworder,110;zoomtowidth,SCREEN_WIDTH"
/>
<Layer
Class="Autoactor"
OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+SCREEN_HEIGHT;decelerate,.25;y,SCREEN_CENTER_Y+SCREEN_HEIGHT/4;"
File="bottom"
InitCommand="draworder,110;zoomtowidth,SCREEN_WIDTH"
/>

<Layer
Class="Autoactor"
File="blueflare"
OnCommand="additiveblend,1;diffusealpha,.6;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomx,15;zoomtoheight,SCREEN_HEIGHT+SCREEN_HEIGHT/4;decelerate,.7	;zoomtoheight,.2;diffusealpha,0;"
InitCommand="draworder,115"
/>



</children>
</ActorFrame>
