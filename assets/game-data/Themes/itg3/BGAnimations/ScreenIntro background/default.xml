	/>
	
	</children></Layer>
	
	
	
	
	
	<Layer
		Class="ActorFrame"
		Command="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+150;"
	><children>
		<Layer
			Class="AutoActor"
			File="swoosh"
