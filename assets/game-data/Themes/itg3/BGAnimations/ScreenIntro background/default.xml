		/>
	</children></Layer>
	
	



	<Layer
		Class="ActorFrame"
		Command="x,SCREEN_CENTER_X;y,SCREEN_TOP+100;rotationZ,-3;sleep,11;linear,6;addx,-10;addy,5"
	><children>
	<Layer
		Class="AutoActor"
		File="blue"
		Command="x,SCREEN_RIGHT+SCREEN_WIDTH*2;zoomtowidth,SCREEN_WIDTH*1.5;zoomtoheight,100;sleep,11;linear,0.5;fadeleft,0.3;faderight,0.3;x,50;linear,4;addx,-100;accelerate,0.5;addx,-SCREEN_WIDTH*2"
	/> 
	
	<Layer
