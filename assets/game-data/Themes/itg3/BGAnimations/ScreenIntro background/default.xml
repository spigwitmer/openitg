<ActorFrame><children>
	<Layer
		File="intro.mpg"
		Command="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_WIDTH/4*3;rate,.92;sleep,35;diffusealpha,0"
	/>
	
	
	<Layer
		Class="Quad"
		Command="vertalign,top;x,SCREEN_CENTER_X;y,SCREEN_TOP;zoomtowidth,SCREEN_WIDTH;zoomtoheight,(SCREEN_WIDTH/4*.35)/2;diffuse,0,0,0,1"
	/>
	<Layer
		Class="Quad"
		Command="vertalign,bottom;x,SCREEN_CENTER_X;y,SCREEN_BOTTOM;zoomtowidth,SCREEN_WIDTH;zoomtoheight,(SCREEN_WIDTH/4*.35)/2;diffuse,0,0,0,1"
	/>
	
	<Layer
		Class="ActorFrame"
		Command="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"
	><children>
		<Layer
			Class="AutoActor"
			File="swoosh"
			Command="y,20;zoomtowidth,SCREEN_WIDTH;additiveblend,1;cropright,1;faderight,1;linear,0.5;cropright,0;faderight,0;sleep,4;linear,0.5;diffusealpha,0.0;addx,50;cropleft,1;fadeleft,1;"
		/>
	</children></Layer>
	<Layer
		Class="BitmapText"
		Font="_v 26px bold glow"
		Text="RoXoR Games Presents"
		Command="additiveblend,1;x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;cropright,1;faderight,1;sleep,.1;linear,.2;cropright,0;faderight,0;sleep,2;linear,0.5;diffusealpha,0"
	/>
	
	<Layer
		Class="BitmapText"
		Font="_v credit"
		Text="RoXoR Games Presents"
		Command="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;cropright,1;faderight,1;sleep,.1;linear,.2;cropright,0;faderight,0;sleep,2;linear,0.5;diffusealpha,0"
	/>
	
	
	
	
	




	<Layer
		Class="ActorFrame"
	><children>
		<Layer
	Class="AutoActor"
	File="wavelogo.xml"
	OnCommand="hibernate,3.2;x,SCREEN_CENTER_X-30;y,SCREEN_CENTER_Y-30;addy,20;addx,30;linear,3.0;sleep,1.2;queuecommand,h"
	hCommand="hidden,1"
	/>
	</children></Layer>

















	
	<Layer
