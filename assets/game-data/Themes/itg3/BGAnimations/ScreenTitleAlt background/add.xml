InitCommand="shadowlength,1;zoomy,.9;zoomx,.8;x,SCREEN_RIGHT-5;y,SCREEN_CENTER_Y+90+37*0"
  OnCommand="horizalign,right;addx,200;sleep,0.9;decelerate,0.25;addx,-200;maxwidth,200;"
/> 
