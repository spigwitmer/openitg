shutupCommand="%function() SHUTUPSERIOUSLY() end"><children>


    <AutoActor
  Condition="AddonPack1()"
  File="_frame"
  InitCommand="x,SCREEN_RIGHT+70;y,SCREEN_CENTER_Y+90+37*0;"
  OnCommand="shadowlength,3;horizalign,right;addx,200;sleep,0.9;decelerate,0.25;addx,-200;PlayCommand,Fade"
  FadeCommand="glow,1,1,1,0;sleep,2;linear,.3;glow,1,1,1,.25;sleep,.05;linear,.5;glow,1,1,1,0;sleep,1;QueueCommand,Fade"
/>
