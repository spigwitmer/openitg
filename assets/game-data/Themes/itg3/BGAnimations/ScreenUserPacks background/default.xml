<ActorFrame>
	<children>
<Layer
File="../_stage background/rear"
InitCommand="zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_HEIGHT"
OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;"
/>
<Layer
File="../_stage background/lines"
InitCommand="zoomtowidth,SCREEN_WIDTH;zoomtoheight,SCREEN_HEIGHT"
OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;diffusealpha,0;sleep,.3;accelerate,0.5;diffusealpha,1"
/>

<Layer
File="cab"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y-70;"
OnCommand="additiveblend,1;diffusealpha,0;sleep,.5;accelerate,0.3;diffusealpha,1"
/>


<Layer
File="slots"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+130;"
OnCommand="diffusealpha,0;zoom,.5;sleep,.8;accelerate,0.3;diffusealpha,1"
/>


<Layer
File="glowslots"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+130;"
OnCommand="diffusealpha,0;zoom,.5;sleep,1.2;queuecommand,Pulse"
PulseCommand="sleep,1;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0"
/>

<Layer
File="usb"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+150;"
OnCommand="addx,70;addy,100;diffusealpha,0;zoom,.5;sleep,1.3;decelerate,1;addx,-70;addy,-100;diffusealpha,1"
/>


<Layer
File="rings"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+64;"
OnCommand="diffusealpha,0;sleep,2.3;accelerate,0.3;diffusealpha,1;sleep,.2;queuecommand,Pulse"
/>

<Layer
File="glowring"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+64;"
OnCommand="diffusealpha,0;sleep,1.2;queuecommand,Pulse"
PulseCommand="sleep,1;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
/>




<Layer
File="pulse"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+70;"
OnCommand="diffusealpha,0;sleep,2.3;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
PulseCommand="sleep,1;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
/>


<Layer
File="pulse"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y+30;"
OnCommand="diffusealpha,0;sleep,2.5;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
PulseCommand="sleep,1;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
/>


<Layer
File="pulse"
InitCommand="x,SCREEN_RIGHT-120;y,SCREEN_CENTER_Y-10;"
OnCommand="diffusealpha,0;sleep,2.7;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
PulseCommand="sleep,1;accelerate,0.3;diffusealpha,1;sleep,.12;decelerate,.8;diffusealpha,0;queuecommand,Pulse"
/>






</children>
</ActorFrame>
